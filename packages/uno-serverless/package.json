    "@types/bcryptjs": "^2.4.1",
    "@types/chai": "^4.1.3",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/mocha": "^5.2.5",
    "@types/msgpack-lite": "^0.1.6",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.7.1",
    "base-58": "0.0.1",
