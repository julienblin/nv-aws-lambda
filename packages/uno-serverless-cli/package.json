{
  "name": "uno-serverless-cli",
  "description": "Command line utility for uno-serverless",
  "version": "0.48.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "bin/",
    "dist/"
  ],
  "bin": {
    "uno": "./bin/uno.js"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "prebuild": "npm run lint",
    "build": "webpack --config webpack.config.js",
    "prepare": "npm run build",
    "test": "mocha",
    "test:watch": "mocha --opts test/mocha.opts --watch",
    "cover": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julienblin/uno-serverless.git"
  },
  "keywords": [],
  "author": "Julien Blin <julien.blin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/julienblin/uno-serverless/issues"
  },
  "homepage": "https://github.com/julienblin/uno-serverless#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/glob": "^5.0.35",
    "@types/js-yaml": "^3.11.2",
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.7.1",
    "@types/yargs": "^11.1.1",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "change-case": "^3.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "glob": "^7.1.2",
    "js-yaml": "^3.12.0",
    "json2yaml": "^1.1.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.9",
    "spawn-sync": "^2.0.0",
    "toposort": "^2.0.2",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typescript": "^3.0.1",
    "typescript-json-schema": "^0.31.0",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.0.8",
    "yargs": "^12.0.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "text",
      "lcov",
      "html"
    ]
  }
}
